{
    "name": "padosoft/laravel-validable",
    "description": "Trait to activate validation when saving Eloquent Model",
    "keywords": [
        "padosoft",
        "laravel-validable"
    ],
    "homepage": "https://github.com/padosoft/laravel-validable",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Padovani",
            "email": "helpdesk@padosoft.com",
            "homepage": "https://www.padosoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/validation": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0|^4.0|^5.0|^6.0|^7.3|^8.0|^9.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0|^8.0|^9.5.20|^10.0|^11.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\Laravel\\Validable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\Laravel\\Validable\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Padosoft\\Laravel\\Validable\\ValidableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
